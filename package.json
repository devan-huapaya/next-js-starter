    "next-auth": "^3.14.6",
    "react": "^16.7.0",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^16.7.0",
    "react-redux": "^7.2.3",
    "redux-persist": "^6.0.0",
    "sass": "^1.32.8"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-plugin-transform-define": "^1.3.0",
    "eslint": "^7.24.0",
