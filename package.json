{
  "name": "genui-next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "next build && next export",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "test:unit": "jest --verbose",
    "test:integration": "jest --verbose --passWithNoTests ./integration",
    "test:e2e": "playwright test",
    "test": "yarn test:unit && yarn test:integration && yarn test:e2e",
    "lint": "next lint",
    "prettier": "prettier --write ./",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "plop": "plop",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset",
    "docker:init": "yarn docker:build:compose && yarn prisma:migrate:dev",
    "build-css": "yarn tailwindcss -i",
    "docker:build:compose": "docker-compose -f docker-compose.yml up -d --build --remove-orphans",
    "docker:build:compose:with-log": "docker-compose -f docker-compose.yml up -d --build --remove-orphans && docker-compose logs -f -t >> docker.log",
    "docker:compose": "docker-compose -f docker-compose.yml up -d && docker-compose logs -f -t >> docker.log"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.14.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "cloudinary": "^1.30.0",
    "daisyui": "^2.15.0",
    "dotenv-cli": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "flowbite": "^1.4.7",
    "form-data": "^4.0.0",
    "hooks-for-redux": "^2.0.3",
    "multer": "^1.4.5-lts.1",
    "next": "12.1.0",
    "next-auth": "^4.3.4",
    "next-connect": "^0.12.2",
    "next-pwa": "^5.4.6",
    "prisma": "^3.14.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-spinners": "^0.11.0",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "remove": "^0.1.5",
    "sharp": "^0.30.5",
    "tailwindcss": "^3.0.24",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "@playwright/test": "^1.19.1",
    "@tailwindcss/forms": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^27.4.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^27.5.1",
    "eslint": "^8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "msw": "^0.41.0",
    "plop": "^3.0.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "./src/**/*.{js,ts,jsx,tsx}": [
      "yarn lint",
      "yarn prettier"
    ]
  }
}
